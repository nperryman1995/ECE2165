#ifndef SRC_ZSCORE_DRIVER_H_
#define SRC_ZSCORE_DRIVER_H_

#include "xil_types.h"

struct Zscore_Control {
	u32 * zscore_baseaddr;
	u32 *g_zscore_data_spots;
	int g_zscore_data_index;
};

void Zscore_setup();
void Zscore_addData(u32 * zscore_baseaddr, u32 data);
u32 Zscore_getStandardDev(u32 * zscore_baseaddr);
u32 Zscore_compareStandardDev(u32 * zscore_baseaddr, u32 threshold);



#endif /* SRC_ZSCORE_DRIVER_H_ */
